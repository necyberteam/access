<?php

use Drupal\views\ViewExecutable;
use Drupal\taxonomy\Entity\Term;

/**
 * Implements hook_views_post_render().
 */
function access_misc_views_post_render(ViewExecutable $view) {
  // Override view /admin/structure/views/view/resources/edit/page_2 title
  if ($view->id() == 'resources' && $view->getDisplay()->view->element['#display_id'] == 'page_2' ) {
    $tid = $view->getTitle();
    $title = term::load($tid)->get('name')->value;
    // Set the view title.
    $view->setTitle($title);
    // Set the route title.
    $route = \Drupal::routeMatch()->getCurrentRouteMatch()->getRouteObject();
    $route->setDefault('_title', $title);
  }
}
